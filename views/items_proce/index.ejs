<h1>Listagem de Itens do Procedimento</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Produto</th>
            <th>Procedimento</th>
            <th>Quantidade</th>
            <th>Ação</th>
        </tr>
    </thead>
    <tbody>
        <% itemsProce.forEach(item => { %>
            <tr>
                <td><%= item.cod %></td>
                <td><%= item.produto_nome %></td>
                <td><%= item.procedimento_nome %></td>
                <td><%= item.quantidade %></td>
                <td>
                    <a href="/items_proce/<%= item.cod %>" class="btn btn-primary" aria-label="Detalhes de <%= item.produto_nome %>">Detalhes</a>
                    <a href="/items_proce/<%= item.cod %>/edit" class="btn btn-warning" aria-label="Editar <%= item.produto_nome %>">Editar</a>
                    <form action="/items_proce/<%= item.cod %>?_method=DELETE" method="POST" style="display: inline;">
                        <button type="submit" class="btn btn-danger" aria-label="Excluir <%= item.produto_nome %>">Excluir</button>
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
<a href="/items_proce/create" class="btn btn-primary">Novo Item do Procedimento</a>
