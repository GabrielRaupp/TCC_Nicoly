<h1>Lista de Feedbacks</h1>

<table class="table">
    <thead>
        <tr>
            <th>Código</th>
            <th>Cliente</th>
            <th>Foto</th>
            <th>Comentário</th>
            <th>Avaliação</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        <% feedbacks.forEach(feedback => { %>
            <tr>
                <td><%= feedback.cod %></td>
                <td><%= feedback.cliente_nome %></td>
                <td><%= feedback.foto %></td>
                <td><%= feedback.comentario %></td>
                <td><%= feedback.avaliacao %></td>
                <td>
                    <a href="/feedbacks/<%= feedback.cod %>" class="btn btn-primary">Ver</a>
                    <a href="/feedbacks/<%= feedback.cod %>/edit" class="btn btn-warning">Editar</a>
                    <form action="/feedbacks/<%= feedback.cod %>?_method=DELETE" method="POST" style="display: inline;">
                        <button type="submit" class="btn btn-danger">Excluir</button>
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
